{% extends 'base.html.twig' %}

{% block title %}4Troll - Register{% endblock %}

{% block body %}
    <h1>Register:</h1>

    {{ form_start(form) }}
    {{ form_row(form.surname) }}
    {{ form_row(form.name) }}
    {{ form_row(form.pseudo) }}
    {{ form_row(form.password)}}
    {{ form_row(form.email) }}
    <button type="submit">Register!</button>
	{{ form_end(form) }}

	

{% endblock %}